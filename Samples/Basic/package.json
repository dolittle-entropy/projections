{
    "private": true,
    "name": "basic",
    "version": "1.0.5",
    "main": "dist/index.js",
    "author": "Dolittle",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --inspect -e ts --exec node -r ts-node/register index.ts",
        "build": "tsc -b"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            "dist",
            "*.d.ts"
        ],
        "events": {
            "restart": "(cd ../../Source;yarn build)"
        },
        "execMap": {
            "ts": "ts-node"
        },
        "watch": [
            "./",
            "../../../Source"
        ],
        "ext": "ts"
    },
    "dependencies": {
        "@dolittle/projections": "1.0.5",
        "@types/express": "^4.17.9",
        "body-parser": "^1.19.0",
        "express": "4.17.1"
    }
}